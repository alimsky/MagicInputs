{
	"name": "Magic Inputs Filler",
	"version": "1.0",
	"description": "This extension allows you to fill up all the inputs on the page, including selects, dropdowns and so on with random values depending on its type.",
	"icons": { "16": "images/icon16.png",
						 "48": "images/icon48.png",
						"128": "images/icon128.png" },
	"browser_action": {
		"default_icon": "images/button.png",
		"default_title": "Fill the inputs"
	},

	"background_page": "background.html",

	"options_page": "options.html",

	"permissions": [
		"tabs", "http://*/*", "https://*/*"
	],

	"content_scripts" : [{
		"matches": [ "<all_urls>" ],
		"js": [ "magicfill.js" ]
	}],
   "update_url": "http://clients2.google.com/service/update2/crx",
  "minimum_chrome_version": "5.0"
}

